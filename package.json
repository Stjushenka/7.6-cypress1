{
  "name": "booksapp",
  "version": "1.0.0",
  "description": "Запуск: npm -i, npm start, npm run server",
  "main": ".eslintrc.js",
  "scripts": {
    "spinning": "forever --minUptime 5000 --spinSleepTime server.js",
    "server": "forever -w server.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "cy:laptop:headless": "npx cypress run -b chrome -C cypress.config.js",
    "cy:laptop:headed": "npx cypress run -b firefox --headed -C cypress.config.js",
    "cy:mobile:headless": "npx cypress run -b edge -C cypress2.config.js",
    "cy:mobile:headed": "npx cypress run -b chrome --headed -C cypress2.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-eslint": "^9.0.0",
    "bootstrap": "^5.2.2",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "cypress": "^10.9.0",
    "eslint": "^5.6.0",
    "forever": "^0.14.2",
    "jest": "^29.1.2",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-router": "^12.0.0",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app": "^1.1.2",
    "react-bootstrap": "^2.5.0",
    "react-component": "^0.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "sudo": "^1.0.3",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "kill-port": "^2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
